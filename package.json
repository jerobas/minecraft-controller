{
  "name": "mine-controller",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node-dev --env-file=.env src/index.ts",
    "build:linux": "tsc && cp -r src/template dist/template",
    "build:win": "tsc && xcopy src\\template dist\\template /E /I",
    "start": "node --env-file=.env dist/index.js"
  },
  "engines": {
    "node": "22.x"
  },
  "keywords": [],
  "author": "flaviozno",
  "license": "ISC",
  "type": "commonjs",
  "description": "Easily set up and manage a Minecraft PAPER server with a clean, user-friendly interface.",
  "dependencies": {
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "minecraft-server-util": "^5.4.4",
    "multer": "^2.0.1",
    "node-cron": "^4.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}